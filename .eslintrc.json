{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off", //unutk menon-aktifkan penggunaan console
        "linebreak-style" : ["error", "windows"], // memperboleh kan bari baru 
        "eol-last": ["error", "never"] // memperbolehkan tidak menggunakan baris baru di ujung code
    }
}
